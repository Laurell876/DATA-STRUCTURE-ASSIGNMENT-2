
public class IterationMathLib extends MathLib {
	
	public int gcd(int x, int y) {
		while (x != y) { 
	        if (x > y)         
	            x = x - y;         
	        else        
	            y = y - x;         
	    } 
	    return x;
	};
	
		
	public int ack(int x, int y) {
		int value[n];
	    size_t size = 0;

	    for (;;) {
	        if (m == 0) {
	            n++;
	            if (size-- == 0) {
	                *status = EXIT_SUCCESS;
	                break;
	            }
	            m = value[size];
	            continue;
	        }

	        if (n == 0) {
	            m--;
	            n = 1;
	            continue;
	        }

	        size_t index = size++;
	        value[index] = m - 1;
	        n--;
	    }

	    return n;
	};
	public int fib(int x) {
		return 1;
	};
	public int hanoi(int n) {
		return 1;
	};

}
